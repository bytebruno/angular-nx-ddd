      "@angular-nx-ddd/customer/feature-favorite-movie": [
        "libs/customer/feature-favorite-movie/src/index.ts"
      ],
      "@angular-nx-ddd/shared/ui-common": ["libs/shared/ui-common/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
